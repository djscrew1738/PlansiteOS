{
  "name": "pipelineos",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered plumbing bid automation platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "api:dev": "npm run dev --workspace=apps/api",
    "worker:dev": "npm run dev --workspace=apps/worker",
    "web:dev": "npm run dev --workspace=apps/web",
    "db:migrate": "npm run migrate --workspace=packages/db",
    "db:seed": "npm run seed --workspace=packages/db",
    "docker:up": "docker-compose -f infra/docker/compose/docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f infra/docker/compose/docker-compose.dev.yml down",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "knex": "^3.1.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctlplumbingllc/pipelineos.git"
  },
  "keywords": [
    "plumbing",
    "ai",
    "automation",
    "blueprint",
    "bid",
    "crm"
  ],
  "author": "CTL Plumbing LLC",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2"
  }
}
